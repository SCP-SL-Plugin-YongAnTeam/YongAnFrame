{
  "metadata": {
    "src": [
      {
        "files": [ "**/*.csproj" ],
        "src": "../"
      }
    ],
    "exclude": [
      "**/bin/**",
      "**/obj/**"
    ],
    "dest": "api"
  },
  "build": {
    "template": [ "default", "modern" ],
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md",
          "toc.yml",
          "*.md"
        ]
      }
    ]
  }
}